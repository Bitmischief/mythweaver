{
  "name": "vue-vite-tailwind",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "validate": "eslint . --ext js,vue && prettier . --check",

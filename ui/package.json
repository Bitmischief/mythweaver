    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",

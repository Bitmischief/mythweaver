    "@formkit/vue": "^1.4.0",
    "@homebaseai/vue3-intercom": "^2.0.2",
    "@tailwindcss/forms": "^0.5.6",
    "axios": "^1.6.1",
    "date-fns": "^2.30.0",
    "gsap": "^3.12.3",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.1.7",
    "pusher-js": "^8.3.0",
    "serve": "^14.2.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.16",

    "@tailwindcss/forms": "^0.5.7",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "gsap": "^3.12.5",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pinia": "^2.1.7",
    "pusher-js": "^8.3.0",
